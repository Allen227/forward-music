
.app-title {
  cursor: pointer;
  font-size: 18px;
}
.menu-list {
  display: flex;
  justify-content: space-between;
  list-style: none;
  cursor: pointer;
  .menu-item {
    padding: 0 24px;
    margin-right: 12px;
    text-decoration: none;
    color: var(--mainFontColor);
    &:hover {
      color: #00BFFF;
    }
  }
  .active {
    background: #00BFFF;
    color: white;
    &:hover {
      color: white;
    }
  }
}