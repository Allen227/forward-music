.app-main {
  background: var(--mainBgColor);
  display: flex;
  .container {
    width: 100%;
    max-width: var(--mainMaxWidth);
    min-width: var(--mainMinWidth);
    height: calc(100vh - 100px);
    margin: 0 auto;
    overflow: hidden;
  }
}
#moment-audio {
  position: absolute;
  right: 20px;
  bottom: 20px;
}